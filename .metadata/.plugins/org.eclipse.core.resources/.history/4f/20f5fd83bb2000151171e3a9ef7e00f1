package GUI;

public class TestString {
	public static void main(String[] args) {
		String s1="abcd";
		String s2=new String ("abcd");
		String s3="ab"+"cd";
		char c1='a';
		char c2='b';
		String s4=c1+c2+"cd";
		System.out.println((s1==s2)+"+"+(s1==s3)+"+"+(s1==s4));
		System.out.println((s2==s3)+"+"+(s2==s4)+"+");
		System.out.println((s3==s4));
	}
}
