package PO;

public class DecisionPO {
	private int priceConst;
	private int distanceConst;
	private String salaryPolicy;
	private String salaryAmount;

	public int getPriceConst() {
		return priceConst;
	}

	public void setPriceConst(int priceConst) {
		this.priceConst = priceConst;
	}

	public int getDistanceConst() {
		return distanceConst;
	}

	public void setDistanceConst(int distanceConst) {
		this.distanceConst = distanceConst;
	}

	public String getSalaryPolicy() {
		return salaryPolicy;
	}

	public void setSalaryPolicy(String salaryPolicy) {
		this.salaryPolicy = salaryPolicy;
	}

	public String getSalaryAmount() {
		return salaryAmount;
	}

	public void setSalaryAmount(String salaryAmount) {
		this.salaryAmount = salaryAmount;
	}

}
